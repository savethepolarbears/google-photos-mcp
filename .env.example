# Google Cloud OAuth credentials
GOOGLE_CLIENT_ID=your_client_id_here
GOOGLE_CLIENT_SECRET=your_client_secret_here
GOOGLE_REDIRECT_URI=http://localhost:3000/auth/callback

# Server configuration
# Port for HTTP mode (change if running multiple dev servers)
# Recommended ports: 3000-3999 for dev services
# Note: If you change PORT, also update GOOGLE_REDIRECT_URI above
PORT=3000
NODE_ENV=development

# MCP server configuration
MCP_SERVER_NAME=google-photos-mcp
MCP_SERVER_VERSION=0.1.0

# Logging
LOG_LEVEL=info

# Token storage path (optional)
TOKEN_STORAGE_PATH=./tokens.json